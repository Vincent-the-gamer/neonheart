{
    "name": "neonheart",
    "displayName": "NeonHeart",
    "description": "An opinionated VS Code theme.",
    "version": "0.1.3",
    "author": "Vincent-the-gamer",
    "publisher": "Vincent-the-gamer",
    "icon": "icon.png",
    "homepage": "https://github.com/Vincent-the-gamer/neonheart#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/Vincent-the-gamer/neonheart"
    },
    "engines": {
        "vscode": "^1.83.0"
    },
    "keywords": [
        "neonheart",
        "cyberpunk"
    ],
    "categories": [
        "Themes"
    ],
    "main": "./dist/index.js",
    "contributes": {
        "themes": [
            {
                "label": "NeonHeart",
                "uiTheme": "vs-dark",
                "path": "./themes/neonheart-color-theme.json"
            }
        ]
    },
    "scripts": {
        "login": "vsce login",
        "package": "vsce package --no-dependencies",
        "publish": "vsce publish --no-dependencies",
        "build": "tsup src/index.ts --external vscode"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@vscode/vsce": "^2.22.0",
        "esno": "^4.0.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}
